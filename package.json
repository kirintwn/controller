{
  "name": "unnamed",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "lint": "eslint . --ext \".js,.jsx,.ts,.tsx\" --fix && tsc",
    "test": "npm run lint && NODE_ENV=test jest",
    "test-update": "npm run lint && NODE_ENV=test jest -u",
    "gulp": "gulp",
    "build-server:utils": "babel src/utils -d build/utils -x \".js,.jsx,.ts,.tsx\" --copy-files",
    "build-server:server": "babel src/server -d build/server -x \".js,.jsx,.ts,.tsx\" --copy-files",
    "build:server": "run-p build-server:*",
    "build:client": "webpack --config webpack.prod.config.js",
    "build": "rm -rf build && run-p build:*",
    "dev:client": "webpack-dev-server --config webpack.dev.config.js",
    "dev": "run-p start dev:client",
    "start": "npm run build:server && node build/server/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && tsc"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": "",
  "license": "MIT",
